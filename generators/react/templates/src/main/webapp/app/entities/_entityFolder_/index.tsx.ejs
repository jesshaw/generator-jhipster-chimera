import React from 'react';
import { Route } from 'react-router-dom';

import ErrorBoundaryRoutes from 'app/shared/error/error-boundary-routes';

import <%= entityReactName %> from './<%= entityFileName %>';
import <%= entityReactName %>Detail from './<%= entityFileName %>-detail';
<%_ if (!readOnly) { _%>
import <%= entityReactName %>Update from './<%= entityFileName %>-update';
<%_ } _%>

const <%= entityReactName %>Routes = () => (
  <ErrorBoundaryRoutes>
    <Route index element={<<%= entityReactName %> />} />
<%_ if (!readOnly) { _%>
    <Route path="new" element={<<%= entityReactName %>Update />} />
<%_ } _%>
    <Route path=":id">
      <Route index element={<<%= entityReactName %>Detail />} />
<%_ if (!readOnly) { _%>
      <Route path="edit" element={<<%= entityReactName %>Update />} />
<%_ } _%>
    </Route>
  </ErrorBoundaryRoutes>
);

export default <%= entityReactName %>Routes;